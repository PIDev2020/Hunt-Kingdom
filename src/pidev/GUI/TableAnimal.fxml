<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pidev.GUI.TableAnimalController">
   <children>
      <TableView fx:id="tablev" layoutX="102.0" layoutY="46.0" prefHeight="264.0" prefWidth="377.0">
        <columns>
            <TableColumn fx:id="coln" prefWidth="75.0" text="Number" />
          <TableColumn fx:id="colr" prefWidth="75.0" text="Race" />
          <TableColumn fx:id="cols" prefWidth="75.0" text="Season" />
            <TableColumn fx:id="colp" prefWidth="75.0" text="Place" />
            <TableColumn fx:id="colref" prefWidth="75.0" text="Reference" />
        </columns>
      </TableView>
      <Button fx:id="btnadd" layoutX="453.0" layoutY="335.0" mnemonicParsing="false" onAction="#addanimal" text="ADD" />
      <Button fx:id="btnmod" layoutX="358.0" layoutY="335.0" mnemonicParsing="false" onAction="#updateAnimal" text="Modify" />
      <Button fx:id="btndel" layoutX="265.0" layoutY="335.0" mnemonicParsing="false" onAction="#deleteAnimal" text="Delete" />
      <TextField fx:id="tfsearch" layoutX="57.0" layoutY="14.0" />
      <Button fx:id="btnref" layoutX="57.0" layoutY="335.0" mnemonicParsing="false" onAction="#refresh" text="Refresh" />
   </children>
</AnchorPane>
